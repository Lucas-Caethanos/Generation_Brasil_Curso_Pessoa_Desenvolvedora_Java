programa
{
	
	funcao inicio()
	{
		cadeia login 
		inteiro senha

	faca{
	escreva("Digite seu login: ")
	leia(login)

	escreva("Digite sua senha: ")
	leia(senha)
	limpa()
	}
	enquanto
	
	se(login == "melissa" e senha == 1234)
		{
			escreva("\nBem Vindes...")	
		}
		senao
		{
			escreva("\nLogin ou senha inválida")
		}
	
	}
}
